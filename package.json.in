{
  "name": "node-nim",
  "version": "${NODE_NIM_VERSION}",
  "description": "NetEase IM nodejs wrapper based on NetEase IM C++ SDK",
  "main": "js/nim.js",
  "types": "types/nim.d.ts",
  "author": "Netease",
  "license": "ISC",
  "homepage": "https://github.com/netease-im/node-nim",
  "repository": {
    "type": "git",
    "url": "git://github.com/netease-im/node-nim.git"
  },
  "scripts": {
    "coverage": "nyc mocha test/test_all.js -slow 200 -timeout 10000",
    "build:ts": "rimraf ./js && rimraf ./types && tsc",
    "build:doc": "typedoc",
    "prepublish": "npm run build:ts",
    "install": "npm run download:sdk",
    "download:sdk": "node download-sdk.mjs"
  },
  "nyc": {
    "cache": false,
    "reporter": [
      "lcov"
    ]
  },
  "dependencies": {
    "download": "^8.0.0",
    "node-fetch": "^2.6.5"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "bluebird": "^3.7.2",
    "cmake-js": "^6.3.0",
    "compare-versions": "^4.1.3",
    "eslint": "^7.32.0",
    "mocha": "^9.1.1",
    "node-addon-api": "^4.3.0",
    "nyc": "^15.1.0",
    "typedoc": "^0.22.13",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "netease",
    "nim",
    "im sdk",
    "nim sdk",
    "nim node",
    "nim electron"
  ],
  "files": [
    "js/",
    "types/",
    "package.json"
  ]
}
