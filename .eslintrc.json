{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-tabs": "off",
        "max-len": [
            "error",
            {
                "code": 150
            }
        ],
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "new-cap": "off",
        "camelcase": "off"
    }
}